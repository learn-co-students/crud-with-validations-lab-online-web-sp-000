<%= form_for @song do |f| %>
    <label>Song title</label><br>
    <%= f.text_field :title %><br>
   
    <%= f.label :artist_name %><br>
    <%= f.text_field :artist_name %><br>
    
    <%= f.label :released %><br>
    <%= f.check_box :released %><br>
  
    <%= f.label :release_year %><br>
    <%= f.text_field :release_year %><br>
   
    <%= f.label :genre %><br>
    <%= f.text_field :genre %><br>
  
    
    <%= f.submit %>
  <% end %>