<%= form_for(@song) do |f| %>
    <p><%= f.label "Title" %>
    <%= f.text_field :title %></p>

    <p><%= f.label "Released" %>
    <%= f.check_box :released %></p>

    <p><%= f.label "Release Year" %>
    <%= f.text_field :release_year %></p>

    <p><%= f.label "Artist Name" %>
    <%= f.text_field :artist_name %></p>

    <p><%= f.label "Genre" %>
    <%= f.text_field :genre %></p>

    <%= f.submit %>
<% end %>