
<%= form_for @song do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :released %>
  <%= f.check_box :released, {}, true, false %>
  <%= f.label :released_false %>
  <%= f.radio_button :released, 'false' %>
  <%= f.label :release_year %>
  <%= f.number_field :release_year %>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <%= f.label :genre %>
  <%= f.text_field :genre %>
  <%= f.submit %>
<% end %>
<%= link_to "Destroy", @song, method: :delete, data: { confirm: "Are you sure?" } %>
