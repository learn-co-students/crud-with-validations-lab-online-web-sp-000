<h1>Edit Song</h1>

<%= form_for(@song) do |s| %> 
    <%= s.label :title %><br>
    <%= s.text_field :title %><br>

    <%= s.label :released %><br>
    <%= s.check_box :released, {}, true, false %><br>

    <%= s.label :release_year %><br>
    <%= s.text_field :release_year %><br>

    <%= s.label :artist_name %><br>
    <%= s.text_field :artist_name %><br>

    <%= s.label :genre %><br>
    <%= s.text_field :genre %><br>


    <%= s.submit %> 
<% end %> 