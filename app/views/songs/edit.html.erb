<h1>Editing <%= @song.title %></h1>
<% if @song.errors.any? %>
<div id='error_explanation'>
  <h2> <%= pluralize(@song.errors.count, "error") %>
     prohibited this article from being saved:
  </h2>
  <ul>
    <% @article.errors.full_messages.each do |msg| %>
     <li><%= msg %></li>
   <% end %>
  </ul>
</div>
<% end %>
<%= form_for(@song) do |f| %>
  <%= f.label :title%>
  <%= f.text_field :title%><br>
  <%= f.label :released%>
  <%= f.check_box :released%><br>
  <%= f.label :year_released%>
  <%= f.number_field :release_year%><br>
  <%= f.label :artist_name%>
  <%= f.text_field :artist_name%><br>
  <%= f.label :genre%>
  <%= f.text_field :genre%><br>
  <br>
  <%= f.submit %>
<% end %>