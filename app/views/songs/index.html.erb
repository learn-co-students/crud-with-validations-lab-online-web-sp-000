<h1> Songs </h1>
<p>
  <table>
    <thead>
      <tr>
        <th> Title </th>
        <th> Artist </th>
        <th> Genre </th>
        <th> Release Year </th>
        <th> Links </th>
      </tr>
    </thead>
    <tbody>
      <% @songs.each do |song| %>
          <tr>
            <td> <%= song.title %></td>          
            <td> <%= song.artist_name %></td>
            <td> <%= song.genre %> </td>
            <td> <%= song.release_year %></td>
            <td>
              <%= link_to 'view', song_path(song) %>
              <%= link_to 'edit', edit_song_path(song) %>
              <%= link_to 'delete', song, method: :delete, data: {confirm: 'Do you really want to delete this song?'} %>
            </td>
          </tr>
      <%end%>
    </tbody>
  </table>
</p>