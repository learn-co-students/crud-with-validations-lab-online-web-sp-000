<h1>All Songs</h1>

<table>
  <tbody>
    <thead>
      <th>Title</th>
      <th>Release Year</th>
      <th>Artist Name</th>
      <th>Genre</th>
    </thead>
    <% @songs.each do |song| %>
      <tr>
        <td><%= link_to song_path(song), song.title %></td>
        <td><%= song.release_year %></td>
        <td><%= song.artist_name %></td>
        <td><%= song.genre %></td>
      </tr>
    <% end %>
  </tbody>
</table>
