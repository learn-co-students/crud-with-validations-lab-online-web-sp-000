<h1>List of Songs</h1>
<table>
    <thead>
        <tr>
            <th>Title</th>
            <th>Released?</th>
            <th>Release Year</th>
            <th>Artist</th>
            <th>Genre</th>
        </tr>
    </thead>
    <tbody>
        <% @songs.each do |song| %>
            <tr>
                <td><%= song.title %></td>
                <td><%= song.released.to_s %></td>
                <td><%= song.release_year.to_s %></td>
                <td><%= song.artist_name %></td>
                <td><%= song.genre %></td>
            </tr>
        <%end%>
    </tbody>
</table>