<table>
  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <td><%= song.title %>
        <td><%= song.release_year %>
        <td><%= song.artist_name %>
        <td><%= song.genre %>
        <td>
          <%= link_to 'View', song %> |
          <%= link_to 'Edit', edit_song_path(song) %> |
          <%= link_to 'Delete', song, method: :delete %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>