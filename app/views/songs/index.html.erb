<h3>Songs</h3>

<table>
  <thead>
    <tr>
    <th>Title</th>
    <th>Artist</th>
    <th>Genre</th>
    </tr>
  </thead>
  <tbody>
    <%@songs.each do |s|%>
      <tr>
        <td><%=s.title%></td>
        <td><%=s.artist_name%><td>
          <td><%=s.genre%></td>
        </tr>
        <%end%>
    </tbody>
</table>
