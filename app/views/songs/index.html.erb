<table>
<tr>
    <th>Title</th>
    <th>Artist</th>
    <th>Genre</th>
    <th>Released</th>
    <th>Release year</th>
    <th>Delete</th>
</tr>
<tbody>
<% @songs.each do |song|%>
    <tr>
        <td><%= song.title %></td>
        <td><%= song.artist_name %></td>
        <td><%= song.genre %></td>
        <td><%= song.released %></td>
        <td><%= song.release_year %></td>
        <td><%= link_to "Delete", song, method: :delete%><td>
    </tr>
<% end %>
</tbody>
</table>