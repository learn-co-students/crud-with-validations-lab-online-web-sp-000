<h1>Songs</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Released</th>
      <th>Release_year</th>
      <th>artist_name</th>
      <th>genre</th>
    </tr>
  </thead>

  <tbody>
  <%= @songs.each do |song| %>
    <tr>
      <td><%= song.title %></td>
      <td><%= 'x' if song.released %></td>
      <td><%= song.release_year %></td>
      <td><%= song.genre %></td>
    </tr>
    <% end %>
  </tbody>
</table>

<%= link_to "New Song", new_song_path %>