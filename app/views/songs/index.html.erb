<% @songs.each do |song|%>
    <table>
        <thead>
            <tr>
                <th>Song Title</th>
                <th>Artist Name</th>
                <th>Genre</th>
                <th>Year Released</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <th><%= song.title %></th>
                <th><%= song.artist_name %></th>
                <th><%= song.genre %></th>
                <th><%= song.release_year %></th>
            </tr>
        </tbody>
    </table>
<% end %>