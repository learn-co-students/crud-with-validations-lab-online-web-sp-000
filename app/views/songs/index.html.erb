<% @songs.each do |song|%>
    <table>
        <tbody>
            <tr>
                <td><%= song.title%></td>
                <td><%= song.release_year%></td>
                <td><%= song.released%></td>
                <td><%= song.genre%></td>
                <td><%= song.artist_name%></td>
            </tr>
        </tbody>
    </table>
<%end%>
