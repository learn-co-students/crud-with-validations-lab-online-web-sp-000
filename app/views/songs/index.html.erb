<h1>All Songs</h1>

<table>
    <thead>
        <tr>
            <th>Title</th>
            <th>Artist Name</th>
            <th>Release Year</th>
        </tr>
    </thead>
    <tbody>
        <% @songs.each do |s| %>
            <tr>
                <td><%= link_to s.title, song_path(s) %></td>
                <td><%= s.artist_name %></td>
                <td><%= s.release_year %></td>
            </tr>
        <% end %>
    </tbody>
</table>

<p><%= link_to "Add New Song", new_song_path %></p>
