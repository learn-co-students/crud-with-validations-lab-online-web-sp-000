<table>
<tbody>
<%= @songs.each do |song| %>
  <tr>
    <td><%= song.title %></td>
    <td><%= song.released ? "Released" : "Not Released" %></td>
    <td><%= song.released ? song.release_year.to_s : "No Release Date" %></td>
    <td><%= song.artist_name %></td>
    <td><%= song.genre %></td>
  </tr>
<% end %>
</tbody>
</table>
