<h1>All Songs</h1>

<table>
    <thead>
        <tr>
        <th>Title</th>
        <th>Artist</th>
        <th>Released</th>
        <th>Release Year</th>
        <th>Genre</th>
        <th colspan="3"></th>
        </tr>
    </thead>

    <tbody>
        <% @songs.each do |song| %>
            <tr>
                <td><%= song.title %></td>
                <td><%= song.artist_name %></td>
                <td><%= song.released %></td>
                <td><%= song.release_year %></td>
                <td><%= song.genre %></td>
                <td><%= link_to 'Show', song %></td>
                <td><%= link_to 'Edit', edit_song_path(song) %></td>
                <td><%= link_to 'Destroy', song, method: :delete, data: { confirm: 'Are you sure?' } %></td>
            </tr>
        <% end %>
    </tbody>
</table>