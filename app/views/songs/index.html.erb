<h1>Songs</h1>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Artist</th>
      <th>Genre</th>
    </tr>
  </thead>
  <tbody>
    <% @songs.each do |s| %>
      <tr>
        <td><%= link_to s.title, s %></td>
        <td><%= s.artist_name %></td>
        <td><%= s.genre %></td>
      </tr>
    <% end %>
  </tbody>
</table>