<h1>Songs</h1>

<table>
  <thead>
    <tr>
      <th>Song Title</th>
      <th>Artist Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <td><%= song.title %></td>
        <td><%= song.artist_name %></td>
        <td><%= link_to "Show", song %></td>
       
      </tr>
    <% end %>
  </tbody>
</table>