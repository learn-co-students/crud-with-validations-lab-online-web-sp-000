<table>
<tr>
<th>Title:</th>
<th>Artist's Name:</th>
<th>Release Year:</th>
<th>Genre:</th>


</tr>

<% @songs.each do |song| %>
    <tr>

    <td><%= song.title %></td>
    <td><%= song.artist_name %></td>
    <td><%= song.release_year %></td>
    <td><%= song.genre %></td>
    
    </tr>
<% end %>

</table>