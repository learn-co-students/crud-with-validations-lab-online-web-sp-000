<h1>All Songs</h1>
<table style="width: 100%; border: 1px solid black" >
  <tr>
    <th>Title</th>
    <th>Released?</th>
    <th>Release Year</th>
    <th>Artist Name</th>
    <th>Genre</th>
  </tr>
  <tbody>
  <% @songs.each do |song| %>
    <tr>
      <td><%= song.title %></td>
      <td><%= song.released %></td>
      <td><%= song.release_year %></td>
      <td><%= song.artist_name %></td>
      <td><%= song.genre %></td>
    </tr>
  <% end %>
  </tbody>
</table>