<%= form_for Song.new do |f| %>
  <%= f.label 'Song Title' %>

  <%= f.text_field :title %>


  <%= f.label 'Song Released?' %>
  <%=  f.check_box :released  %>

  <%= f.label 'Song Genre' %>
  <%= f.text_field :genre  %>

  <%= f.label 'Song Artist Name' %>
  <%= f.text_field :artist_name  %>

  <%= f.label 'Song Release Year' %>
  <%= f.text_field :release_year %>


  <%= f.submit "Submit Song" %>

  <% end %>
