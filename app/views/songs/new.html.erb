<h1>Create a New Song</h1>

<%= form_for(@song) do |f| %>

  <label>Title:</label>
    <%= f.text_field :title %>

  <label>Released:</label>
    <%= f.text_field :released %>

  <label>Released Year:</label>
    <%= f.text_field :release_year %>

    <label>Artist Name:</label>
      <%= f.text_field :artist_name %>

  <label>Genre:</label>
    <%= f.text_field :genre %>

  <%= f.submit "Create Song" %>

<% end %>
