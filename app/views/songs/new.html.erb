<h1> Create your song Here </h1>

<%= form_for @song do |f| %>
<%=f.label "Song title" %>
<%=f.text_field :title %>
<%=f.label "Released"  %>
<%= f.check_box :released, {}, true, false %>
<%=f.label "Release year "%>
<%=f.number_field :release_year %>
<%=f.label "Artist Name" %>
<%=f.text_field :artist_name %>
<%=f.label :"Genre" %>
<%=f.text_field :genre %>
<%=f.submit %>
<% end%>
