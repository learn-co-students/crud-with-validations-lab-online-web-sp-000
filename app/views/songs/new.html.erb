<h1>Create Song Form</h1>
<ul>
<%= form_for(@song) do |f| %>
    <li>Title: <%= f.text_field :title %></li><br>
    <li>Released? <%= f.check_box :released %></li><br>
    <li>Release Year: <%= f.number_field :release_year %></li><br>
    <li>Artist Name: <%= f.text_field :artist_name %></li><br>
    <li>Genre: <%= f.text_field :genre %></li><br>
    <%= f.submit %>
<% end %>
</ul>